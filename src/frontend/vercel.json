{
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/old-path",
            "destination": "/new-path",
            "permanent": true
        }
    ],
    "functions": {
        "api/**.js": {
            "memory": 128,
            "maxDuration": 10
        }
    },
    "headers": [
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}


